import java.io.*;
public class Work {
	public static void main(String[] args)throws IOException{
		File file = new File("C:/Users/Happy Kohli/Desktop");
		FileReader f= new FileReader(file);
		try{
			int m;
			while((m=f.read())!=10){
				System.out.print((char)m);
				}
			
			
			
			
			
			
		}catch(Exception e){
			System.out.println("Error Occured");
		}
		finally{
			if(f!=null){
				f.close();
			}
		}
	}

}
