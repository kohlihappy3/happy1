
public class Class2_Palindrome_HappyKohli {
	public static void main(String[] args)
	{
		int num=1221;
		int a,sum=0;
		
		
		while(num>0)
		{
			a=num%10;
			sum=(sum*10)+a;
			num=num/10;
			
		}
		if(num==sum)
			System.out.print(" The Number Is Palindrome"+num);
		else
			System.out.print(" The Number Is Not A Palindrome"+num);
	}

}
