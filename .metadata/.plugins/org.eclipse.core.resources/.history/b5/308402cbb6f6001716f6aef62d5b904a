
public class Class2_GreatNo_HappyKohli {
	public static void main(String[] args)
	{
		int a=8;
		int b=2;
		int c=1;
		if(a>b && a>c)
			System.out.print("a is the greatest number"+a);
		else if(b>a && b>c)
			System.out.print("b is the greatest number"+b);
		else
			System.out.print("c is the greatest number"+c);
	}

}
