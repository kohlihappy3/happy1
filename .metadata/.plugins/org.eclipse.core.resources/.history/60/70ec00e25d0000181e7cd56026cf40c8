
public class kuchB {
	public static void main(String[] args)
	{
		int arr[]={12,5,26,35,4,9,22,78,1};
		int count=1;
		for(int i=0;i<=arr.length;i++)
		{
			for(int j=0;j<arr.length;j++)
			{
				if(arr[j]<arr[j+1]){
					count++;
					}else{
						break;
					}
			}
			System.out.print(count);
		}
	}

}
