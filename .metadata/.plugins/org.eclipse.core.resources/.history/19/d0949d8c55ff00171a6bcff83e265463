
public class ComplexOp {
	int a,b;
	public ComplexOp(int real, int img)
	{
		a=real;
		b=img;
	}
	void disp()
	{
		System.out.println(" The Number is :"+a+"i"+b);
	}
	ComplexOp add(ComplexOp x)
	{
		ComplexOp c=new ComplexOp();
		c.a=this.a+x.a;
		c.b=this.b+x.b;
		return x;
	}
	ComplexOp sub(ComplexOp x)
	{
		ComplexOp c=new ComplexOp();
		c.a=this.a-x.a;
		c.b=this.b-x.b;
		return x;
	}
	/*ComplexOp mult(ComplexOp x)
	{
		ComplexOp c=new ComplexOp();
		c.a=this.a+x.a;
		c.b=this.b+x.b;
		return x;
	}*/
	
	}


