
public class Complex {
	int a,b;
	public Complex(int real, int img)
	{
		a=real;
		b=img;
	}
	public Complex()
	{
		a=0;
		b=0;
	}
	void disp()
	{
		System.out.println(" The Number is :"+a+"+"+b+"i");
	}
	Complex add(Complex x)
	{
		Complex c=new Complex();
		c.a=this.a+x.a;
		c.b=this.b+x.b;
		return x;
	}
	Complex sub(Complex x)
	{
		Complex c=new Complex();
		c.a=this.a-x.a;
		c.b=this.b-x.b;
		return x;
	}
	/*ComplexOp mult(ComplexOp x)
	{
		ComplexOp c=new ComplexOp();
		c.a=this.a+x.a;
		c.b=this.b+x.b;
		return x;
	}*/
	
	}


