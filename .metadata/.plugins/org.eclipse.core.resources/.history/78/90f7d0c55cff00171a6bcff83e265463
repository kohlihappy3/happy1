
public class kkk {
	
			public static void main(String[]args){
			Complex co1=new Complex(8,13);
			Complex co2=new Complex(10,5);
			Complex result=new Complex();
			System.out.println("The Addition Of Complex Numbers are:");
			result =co1.add(co2);
			result.disp();
			System.out.println("The Substraction of Numbers are:");
			result=co1.sub(co2);
			result.disp();
		}
}
